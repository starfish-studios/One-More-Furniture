{
    "multipart": [
        {
            "apply": {
                "model": "nookcranny:block/table/warped_top"
            }
        },
        {
            "when": {
                "leg_1": "true"
            },
            "apply": {
                "model": "nookcranny:block/table/warped_leg",
                "uvlock": false
            }
        },
        {
            "when": {
                "leg_2": "true"
            },
            "apply": {
                "model": "nookcranny:block/table/warped_leg",
