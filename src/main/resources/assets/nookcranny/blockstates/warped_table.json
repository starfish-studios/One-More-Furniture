                "uvlock": false
            }
        },
        {
            "when": {
                "leg_3": "true"
            },
            "apply": {
                "model": "nookcranny:block/table/warped_leg",
                "y": 180,
                "uvlock": false
            }
        },
        {
            "when": {
                "leg_4": "true"
            },
            "apply": {
                "model": "nookcranny:block/table/warped_leg",
                "y": 270,
                "uvlock": false
            }
        }
    ]
